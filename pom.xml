    <groupId>com.epam.shuvaev.mentoring</groupId>
    <artifactId>com.epam.shuvaev.mentoring</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>
    <modules>
        <module>troubleshooting</module>
        <module>creation-pattern</module>
        <module>web-services-task2</module>
        <module>web-services-task3</module>
        <module>web-services-task1</module>
